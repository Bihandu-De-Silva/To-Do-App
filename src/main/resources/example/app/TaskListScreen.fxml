<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="700.0" style="-fx-background-color: #72C9E0;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="example.app.TaskListController">
        <children>
            <ListView fx:id="taskListView" layoutX="50.0" layoutY="50.0" prefHeight="300.0" prefWidth="600.0" />
            <Button layoutX="330.0" layoutY="360.0" onAction="#handleBack" style="-fx-background-color: #2196F3;" text="Back">
         <font>
            <Font name="Tahoma" size="13.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
        </children>
</AnchorPane>
